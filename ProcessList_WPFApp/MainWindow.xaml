<Window x:Class="ProcessList_WPFApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProcessList_WPFApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="200">
    <Grid Background="#F2F2F2">
        <Button Content="Launch Notepad" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,20,0,0" Padding="10" Width="150" Click="LaunchNotepad_Click"/>
        <Button Content="Launch Calculator" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,80,0,0" Padding="10" Width="150" Click="LaunchCalculator_Click"/>
        <Button Content="Launch Paint" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,140,0,0" Padding="10" Width="150" Click="LaunchPaint_Click"/>
        <Button Content="Launch Spotify" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,200,0,0" Padding="10" Width="150" Click="LaunchSpotify_Click"/>
    </Grid>
</Window>
